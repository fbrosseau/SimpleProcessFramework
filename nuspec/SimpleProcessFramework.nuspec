<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>SimpleProcessFramework</id>
    <version>0.0.46</version>
    <description>A library to easily spawn subprocesses across multiple machines, multiple .Net frameworks and multiple operating systems</description>
    <authors>francis@malagauche.com</authors>
    <dependencies>
      <dependency id="System.IO.FileSystem.AccessControl" version="4.5.0" />
      <dependency id="System.Memory" version="4.5.2" />
      <dependency id="System.Reflection.Emit" version="4.3.0" />
      <dependency id="System.Reflection.Emit.ILGeneration" version="4.3.0" />
      <dependency id="System.Reflection.Emit.Lightweight" version="4.3.0" />
      <dependency id="System.Threading.Tasks.Extensions" version="4.5.2" />
    </dependencies>
  </metadata>
  <files>
    <file src="SimpleProcessFramework.targets" target="build" />
    <file src="../bin/$configuration$/Spfx.dll" target="lib/netstandard2.0" />
    <file src="../bin/$configuration$/Spfx.pdb" target="lib/netstandard2.0" />
    <file src="../bin/$configuration$/netcoreapp2.1/Spfx.Process.Netcore.dll" target="tools/netcoreapp2.1" />
    <file src="../bin/$configuration$/netcoreapp2.1/Spfx.Process.Netcore.pdb" target="tools/netcoreapp2.1" />
    <file src="../bin/$configuration$/netcoreapp2.1/Spfx.Process.Netcore.deps.json" target="tools/netcoreapp2.1" />
    <file src="../bin/$configuration$/netcoreapp2.1/Spfx.Process.Netcore.runtimeconfig.json" target="tools/netcoreapp2.1" />

    <file src="../bin/$configuration$/net472/Spfx.Process.Netfx.exe" target="tools/net472" />
    <file src="../bin/$configuration$/net472/Spfx.Process.Netfx.pdb" target="tools/net472" />
    <file src="../bin/$configuration$/net472/Spfx.Process.Netfx32.exe" target="tools/net472" />
    <file src="../bin/$configuration$/net472/Spfx.Process.Netfx32.pdb" target="tools/net472" />
  </files>
</package>